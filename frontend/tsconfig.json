{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "composite": false,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"]
  },
  "include": ["src/**/*", "env.d.ts"]
}
